/**app.wxss**/
page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;    
  margin: 0;
  box-sizing: border-box;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
  background: #eeeeee;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  /* height:100%; */
}